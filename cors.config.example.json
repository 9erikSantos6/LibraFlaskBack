{
    "development": {
        "resources": {
            "/*": {
                "origins": ["*"]
            }
        },
        "supports_credentials": true,
        "allow_headers": ["Content-Type", "Authorization"],
        "expose_headers": ["Content-Type", "Authorization"],
        "max_age": 3600
    },
    "production": {
        "resources": {
            "/*": {
                "origins": ["https://example-front.com"]
            }
        },
        "supports_credentials": true,
        "allow_headers": ["Content-Type", "Authorization"],
        "expose_headers": ["Content-Type", "Authorization"],
        "max_age": 3600
  }
}